{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "ryegoryshev/multi-nginx",
            "hostname": "nginx-proxy",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "memory": 128
        }
    ]
}